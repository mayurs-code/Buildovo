<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="BottomNavigationTheme" parent="Widget.Design.BottomNavigationView">
        <item name="android:colorPrimary">@color/colorPrimary</item>
        <item name="android:colorPressedHighlight">@color/colorPrimaryDark</item>

    </style>

    <style name="Header_button_style">
        <item name="android:colorPrimary">@color/BottomNavigationView</item>

    </style>


</resources>
